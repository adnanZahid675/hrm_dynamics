{
  "name": "mantine-vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "serve -s dist",
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "icons": "npx @svgr/cli --typescript --icon 20 --ignore-existing --out-dir src/assets/iconComponents --replace-attr-values '#000=currentColor' --index-template src/shared/svgr/template.js -- src/assets/icons",
    "analyze": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.2.0",
    "@azure/msal-react": "^3.0.4",
    "@hookform/resolvers": "^3.3.4",
